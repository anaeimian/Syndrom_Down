<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar" />


            <ImageView
                android:id="@+id/imViewFel"
                android:layout_width="@dimen/jomle_item_dim"
                android:layout_height="@dimen/jomle_item_dim"
                android:src="@drawable/fel"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="@dimen/jomle_item_left_margin"
                android:layout_marginStart="@dimen/jomle_item_left_margin"
                android:layout_below="@+id/app_bar"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageView
                android:id="@+id/imView2Kalame"
                android:layout_width="@dimen/jomle_item_dim"
                android:layout_height="@dimen/jomle_item_dim"
                android:src="@drawable/dok"
                android:layout_gravity="center_horizontal"
                android:layout_below="@+id/app_bar"
                android:layout_toRightOf="@+id/imViewFel"
                android:layout_toEndOf="@+id/imViewFel"
                android:layout_marginLeft="114dp"
                android:layout_marginStart="114dp" />

            <ImageView
                android:id="@+id/imView3Kalame"
                android:layout_width="@dimen/jomle_item_dim"
                android:layout_height="@dimen/jomle_item_dim"
                android:src="@drawable/sek"
                android:layout_below="@+id/imViewFel"
                android:layout_toRightOf="@+id/imViewFel"
                android:layout_toEndOf="@+id/imViewFel" />

            <ImageView
                android:id="@+id/imViewZamir"
                android:layout_width="@dimen/jomle_item_dim"
                android:layout_height="@dimen/jomle_item_dim"
                android:src="@drawable/zamir"
                android:layout_below="@+id/imView2Kalame"
                android:layout_toRightOf="@+id/imView2Kalame"
                android:layout_toEndOf="@+id/imView2Kalame" />

</RelativeLayout>